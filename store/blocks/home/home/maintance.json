{
    "flex-layout.row#mantenimiento": {
        "children":[
            "flex-layout.col#mantenimiento"
        ],
        "props":{
             "blockClass":"imageMantenimiento"
        }
    },
    "flex-layout.col#mantenimiento": {
        "children":[
            "responsive-layout.desktop#mantenimiento",
            "responsive-layout.mobile#mantenimiento"
        ],
        "props":{
             "blockClass":"imageMantenimiento"
        }
    },
    "responsive-layout.desktop#mantenimiento": {
        "children": [
            "image#mantenimiento"
        ]
    },
    "responsive-layout.mobile#mantenimiento": {
        "children": [
        "image#mantenimientoMobile"
        ]
    },
    "image#mantenimiento": {
        "props": {
            "src": "/arquivos/BANNER-WEB-EN-SERVICIO-100.jpg",
            "width": "100%",
            "blockClass":"imageMantenimiento"
        }
    },
    "image#mantenimientoMobile": {
        "props": {
            "src": "/arquivos/imageMantenimientoMobile.jpeg",
            "width": "100%",
            "blockClass":"imageMantenimiento"
        }
    }
}